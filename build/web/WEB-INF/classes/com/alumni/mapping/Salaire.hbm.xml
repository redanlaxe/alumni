<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.alumni.mapping.Salaire" table="SALAIRE" schema="ALUMNI">
        <id name="idsalaire" type="int">
            <column name="IDSALAIRE" />
            <generator class="identity" />
        </id>
        <property name="idexperience" type="java.lang.Integer">
            <column name="IDEXPERIENCE" />
        </property>
        <property name="valeur" type="java.lang.Double">
            <column name="VALEUR" precision="52" scale="0" />
        </property>
        <property name="annee" type="java.lang.Integer">
            <column name="ANNEE" />
        </property>
    </class>
</hibernate-mapping>
